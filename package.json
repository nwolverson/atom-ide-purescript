{
  "name": "ide-purescript",
  "main": "./lib/main",
  "version": "0.8.2",
  "description": "PureScript support for completion, build and maybe more",
  "keywords": [],
  "activationHooks": [
    "language-purescript:grammar-used"
  ],
  "repository": "https://github.com/nwolverson/atom-ide-purescript",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "^3.0.0",
    "atom-space-pen-views": "^2.1.0",
    "glob": "^5.0.14",
    "hover-tooltips": "git+http://github.com/nwolverson/hover-tooltips.git#scrolltop-removed",
    "xregexp": "~2.0.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "1.0.0": "consumeLinterIndie"
      }
    },
    "linter-plus-self": {
      "versions": {
        "0.1.0": "consumeLinterInternal"
      }
    }
  }
